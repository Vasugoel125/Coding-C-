#include<bits/stdc++.h>
using namespace std;
int main()
{
  int n,temp,rev=0;
  cin>>n;
  int original=n;
  while(n>0)
  {
     temp=n%10;
     rev=(rev*10)+temp;
     n=n/10;
  }
  if(original==rev)
  {
     cout<<"The number "<<original<<" is palindrome";
  }
  else
  {
     cout<<"The number "<<original<<" is not palindrome";
  }
  return 0;
}
